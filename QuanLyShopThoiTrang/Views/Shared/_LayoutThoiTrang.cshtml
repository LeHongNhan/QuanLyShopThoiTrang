<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <style>
        .dropdown:hover .dropdown-menu {
            
        }
        .nav-item .dropdown-menu .dropdown  .dropdown-item:hover{
            background-color:#808080;
            transition: top 0.3s ease;
            color:yellow;
        }
        .nav-item .dropdown-menu .dropdown .dropdown-menu{
            z-index:1;
            background-color:#e9e8e8;
            position: absolute;
            display: none;
        }
          .nav-item .dropdown-menu .dropdown:hover > .dropdown-menu {
            top:-10%;
            display: block;
        }
          .nav-item .dropdown-menu .dropdown .dropdown-menu .dropdown-item:hover {
            transition: top 0.3s ease;
            color:yellow;
            background-color:#808080;
        }
       .carousel-inner {
            display: flex;
            flex-wrap: nowrap;
        }

        
    </style>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" rel="stylesheet">
    <!--Bootstrap 4-->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" />
    <link href="~/Asset/styleNonGiay.css" rel="stylesheet" />
</head>
<body>
    <div class="container-fluid">
        @RenderPage("/Views/Shared/_PartialHeader.cshtml")
        @RenderPage("/Views/Shared/_PartialPanel.cshtml")
        @RenderBody()
        @RenderPage("/Views/Shared/_PartialFooter.cshtml")
    </div>
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

</body>
</html>
