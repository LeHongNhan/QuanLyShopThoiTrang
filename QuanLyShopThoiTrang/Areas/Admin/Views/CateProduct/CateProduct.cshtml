@model IEnumerable<QuanLyShopThoiTrang.Models.Category>

@{
    ViewBag.Title = "ProductVariantPartials";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayoutPage1.cshtml";
}
<h2>Cate Product</h2>

<p>
    @Html.ActionLink("Create New", "AddCate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.category_name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.category_name)
            </td>
            <td>
                @Html.ActionLink("Update", "UpdateCate", new { id = item.category_id }) |
                @Html.ActionLink("Delete", "DeleteCate", new { id = item.category_id })
            </td>
        </tr>
    }

</table>
